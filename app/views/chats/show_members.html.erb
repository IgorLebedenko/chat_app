<% provide(:title, @title) %>
<h1><%= @title %></h1>
<h2>Members:</h2>


<%= will_paginate @members %>
<ul class="users">
  <% @members.each do |member| %>
    <li>
      <div class="image">
        <%= avatar_for member, size: 52 %>
        <% status = @current_user.online? ? "online" : "offline" %>
        <div class="<%= status %>"></div>
      </div>
      <%= link_to member.name, member %>
    </li>
  <% end %>
</ul>
<%= will_paginate @members%>